{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "BootstrapServers": "kafka:9092",
    "Topic": "logs"
  },
  "HostFiltering": {
    "AllowedHosts": [ "localhost:5000", "userservice", "authservice", "logservice" ]
  }
}
