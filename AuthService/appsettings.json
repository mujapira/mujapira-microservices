{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Kafka": {
    "BootstrapServers": "kafka:9092",
    "Topic": "auth"
  },
  "Services": {
    "ApiGateway": "http://apigateway:5000"
  },
  "HostFiltering": {
    "AllowedHosts": [ "localhost:5000", "userservice", "authservice", "logservice" ]
  },
  "CsrfTokenStore": {
    "CsrfTokenTtlMinutes": 43200
  }
}